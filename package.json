    "vue-router": "^4.4.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.15.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-vue": "^9.31.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^26.6.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "vite": "^5.4.10",
